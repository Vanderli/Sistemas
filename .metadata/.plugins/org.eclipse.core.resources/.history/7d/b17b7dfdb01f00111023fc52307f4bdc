  `sec_bairro` varchar(255) NOT NULL,
  `sec_cidade` varchar(45) NOT NULL,
  `sec_tel_1` varchar(9) NOT NULL,
  `sec_tel_2` varchar(9) NOT NULL,
  `sec_tel_3` varchar(9) NOT NULL,
