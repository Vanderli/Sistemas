  KEY `contato_sec_FKIndex1` (`con_sec_id`),
  KEY `contato_sec_FKIndex2` (`con_usu_id`)
) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=latin1;

