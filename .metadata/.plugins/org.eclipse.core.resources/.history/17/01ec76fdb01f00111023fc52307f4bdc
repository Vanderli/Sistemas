  `imp_status` enum('ATIVO','INATIVO') default NULL,
  PRIMARY KEY  (`imp_id`),
  KEY `importante_FKIndex1` (`imp_usu_id`),
  KEY `importante_FKIndex2` (`imp_not_id`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=latin1;

