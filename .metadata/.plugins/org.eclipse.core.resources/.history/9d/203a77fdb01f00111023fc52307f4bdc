  `ipo_status` enum('ATIVO','INATIVO') NOT NULL,
  PRIMARY KEY  (`ipo_oficial`),
  KEY `imprensa_oficial_FKIndex1` (`ipo_usu_id`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

