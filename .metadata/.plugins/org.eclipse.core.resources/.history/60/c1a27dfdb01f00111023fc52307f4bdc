  `atr_dat_cad` date NOT NULL,
  `atr_hora_cad` time NOT NULL,
  `atr_titulo` varchar(255) NOT NULL,
  `atr_desc` text NOT NULL,
  `atr_status` enum('ATIVO','INATIVO') NOT NULL,
  PRIMARY KEY  (`atr_id`),
